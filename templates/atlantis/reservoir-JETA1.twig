{% extends 'layout.html.twig' %}
{% block title %} {{ "Réservoir JET A1"|trans }} - {{ "Atlantis Cameroun"|trans }}{% endblock %}
{% block gallery %} active {% endblock %}
{% block photo %} active {% endblock %}
